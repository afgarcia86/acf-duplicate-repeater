      "build": [
        "gulp build"
      ]
    },
    "git": {
      "host": "github.com",
      "user": "mcguffin"
    },
    "wporg": {
      "assets": ".wporg",
      "svn": "https://plugins.svn.wordpress.org/acf-duplicate-repeater/"
    }
  }
